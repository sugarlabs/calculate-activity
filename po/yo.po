#: functions.py:112
msgid "abs(x), return absolute value of x, which means -x for x < 0"
msgstr "abs(x), pada idi iye ti x, eyi ti o tumo -x fun x < 0"

#: functions.py:117
msgid ""
"acos(x), return the arc cosine of x. This is the angle for which the cosine "
"is x. Defined for -1 <= x < 1"
msgstr ""
"acos(x), pada awọn aaki cosine ti x. Eleyi ni awọn igun fun eyi ti awọn "
"cosine jẹ x. Telẹ fun -1 <= x < 1"

#: functions.py:123
msgid ""
"acosh(x), return the arc hyperbolic cosine of x. This is the value y for "
"which the hyperbolic cosine equals x."
msgstr ""
"acosh (x), pada awọn aaki hyperbolic cosine ti x. Eleyi jẹ iye y fun eyi ti "
"awọn hyperbolic cosine je egbe x."

#: functions.py:129
msgid ""
"And(x, y), logical and. Returns True if x and y are True, else returns False"
msgstr ""
"Ati (x, y), mogbonwa ati ki o. Pada Otitọ ti o ba ti x ati y ni o wa Otitọ, "
"miran pada Eke"

#: functions.py:136
msgid "add(x, y), return x + y"
msgstr "fi (x, y), pada x + y"

#: functions.py:141
msgid ""
"asin(x), return the arc sine of x. This is the angle for which the sine is "
"x. Defined for -1 <= x <= 1"
msgstr ""
"Asin (x), pada awọn aaki lai ti x. Eleyi ni awọn igun fun eyi ti awọn lai ni "
"x. Telẹ fun -1 <= x <= 1"

#: functions.py:147
msgid ""
"asinh(x), return the arc hyperbolic sine of x. This is the value y for which "
"the hyperbolic sine equals x."
msgstr ""
"asinh (x), pada awọn aaki hyperbolic lai ti x. Eleyi jẹ iye y fun eyi ti "
"awọn hyperbolic lai je egbe x."

#: functions.py:153
msgid ""
"atan(x), return the arc tangent of x. This is the angle for which the "
"tangent is x. Defined for all x"
msgstr ""
"Atan (x), pada awọn aaki tangent ti x. Eleyi ni awọn igun fun eyi ti awọn "
"tangent ni x. Telẹ fun gbogbo x"

#: functions.py:159
msgid ""
"atanh(x), return the arc hyperbolic tangent of x. This is the value y for "
"which the hyperbolic tangent equals x."
msgstr ""
"atanh (x), pada awọn aaki hyperbolic tangent ti x. Eleyi jẹ iye y fun eyi ti "
"awọn hyperbolic tangent je egbe x."

#: functions.py:171
msgid "Number does not look binary in base 10"
msgstr "Number ko ni wo alakomeji ni mimọ 10"

#: functions.py:178
msgid ""
"b10bin(x), interpret a number written in base 10 as binary, e.g.: b10bin"
"(10111) = 23,"
msgstr ""
"b10bin (x), itumọ nọmba kan kọ ni mimọ 10 bi alakomeji, eg: b10bin(10111) = "
"23,"

#: functions.py:183
msgid "ceil(x), return the smallest integer larger than x."
msgstr "ceil (x), pada awọn kere odidi o tobi ju x."

#: functions.py:188
msgid ""
"cos(x), return the cosine of x. This is the x-coordinate on the unit circle "
"at the angle x"
msgstr ""
"nitori(x), pada awọn cosine ti x. Eleyi jẹ awọn x-ipoidojuko lori kuro "
"circle ni igun x"

#: functions.py:194
msgid ""
"cosh(x), return the hyperbolic cosine of x. Given by (exp(x) + exp(-x)) / 2"
msgstr ""
"cosh (x), pada awọn hyperbolic cosine ti x. Fun nipasẹ (exp (x) + exp (-x)) /"
" 2"

#: functions.py:198
msgid "Can not divide by zero"
msgstr "Ko le pin nipa odo"

#: functions.py:219
msgid "Invalid argument"
msgstr "Invalid ariyanjiyan"

#: functions.py:222
msgid ""
"gcd(a, b), determine the greatest common denominator of a and b. For "
"example, the biggest factor that is shared by the numbers 15 and 18 is 3."
msgstr ""
"gcd (a, b), mọ awọn ti o tobi wọpọ iyeida kan ti a ti ati b. Fun apẹẹrẹ, "
"awọn tobi ifosiwewe ti wa ni pín nipasẹ awọn nọmba 15 ati 18 ti wa ni 3."

#: functions.py:227
msgid "exp(x), return the natural exponent of x. Given by e^x"
msgstr "exp(x), pada awọn adayeba agbènọmbàga ti x. Fun nipasẹ e^x"

#: functions.py:231
msgid "Factorial only defined for integers"
msgstr "Factorial nikan telẹ fun odidi"

#: functions.py:244
msgid ""
"factorial(n), return the factorial of n. Given by n * (n - 1) * (n - 2) * ..."
msgstr ""
"factorial (n), pada awọn factorial ti n. Fun nipasẹ n * (n - 1) * (n - 2) * "
"..."

#: functions.py:250
msgid "fac(x), return the factorial of x. Given by x * (x - 1) * (x - 2) * ..."
msgstr "Ṣe(x), pada awọn factorial ti x. Fun nipasẹ x * (x - 1) * (x - 2) * ..."

#: functions.py:283
msgid "floor(x), return the largest integer smaller than x."
msgstr "pakà (x), pada awọn ti odidi kere ju x."

#: functions.py:287
msgid "inv(x), return the inverse of x, which is 1 / x"
msgstr "inv (x), pada awọn onidakeji ti x, ti o jẹ 1 / x"

#: functions.py:309 functions.py:318
msgid "Logarithm(x) only defined for x > 0"
msgstr "Logarithm (x) nikan telẹ fun x> 0"

#: functions.py:311
msgid ""
"ln(x), return the natural logarithm of x. This is the value for which the "
"exponent exp() equals x. Defined for x >= 0."
msgstr ""
"ln(x), pada awọn adayeba logarithm ti x. Eleyi jẹ iye fun eyi ti awọn "
"agbènọmbàga exp() je egbe x. Telẹ fun x >= 0."

#: functions.py:320
msgid ""
"log10(x), return the base 10 logarithm of x. This is the value y for which "
"10^y equals x. Defined for x >= 0."
msgstr ""
"log10(x), pada awọn mimọ 10 logarithm ti x. Eleyi jẹ iye y fun eyi ti 10^y "
"je egbe x. Telẹ fun x >= 0."

#: functions.py:327
msgid "Can only calculate x modulo <integer>"
msgstr "Le nikan iṣiro x ìfiwọn <odidi>"

#: functions.py:329
msgid ""
"mod(x, y), return the modulus of x with respect to y. This is the remainder "
"after dividing x by y."
msgstr ""
"moodi (x, y), pada awọn modulus ti x pẹlu ọwọ lati y. Eleyi jẹ awọn ku lẹhin "
"ti pin x nipa y."

#: functions.py:337
msgid "mul(x, y), return x * y"
msgstr "mul (x, y), pada x * y"

#: functions.py:341
msgid "negate(x), return -x"
msgstr "negate (x), pada -x"

#: functions.py:346
msgid ""
"Or(x, y), logical or. Returns True if x or y is True, else returns False"
msgstr ""
"Tabi (x, y), mogbonwa tabi. Pada Otitọ ti o ba ti x tabi y ni Otitọ, miran "
"pada Eke"

#: functions.py:361
msgid "pow(x, y), return x to the power y (x**y)"
msgstr "pow (x, y), pada x to agbara y (x**y)"

#: functions.py:366
msgid "rand_float(), return a random floating point number between 0.0 and 1.0"
msgstr "rand_float(), pada a ID lilefoofo ojuami nọmba laarin awọn 0,0 ati 1.0"

#: functions.py:371
msgid ""
"rand_int([<maxval>]), return a random integer between 0 and <maxval>. "
"<maxval> is an optional argument and is set to 65535 by default."
msgstr ""
"rand_int ([<maxval>]), pada a ID odidi laarin 0 ati <maxval>. <maxval> jẹ "
"ẹya iyan ariyanjiyan ati ki o ti ṣeto si 65535 nipa aiyipada."

#: functions.py:376
msgid "round(x), return the integer nearest to x."
msgstr "yika (x), pada odidi sunmọ to x."

#: functions.py:382 functions.py:390
msgid "Bitwise operations only apply to integers"
msgstr "Bitwise mosi nikan waye si odidi"

#: functions.py:384
msgid "shift_left(x, y), shift x by y bits to the left (multiply by 2 per bit)"
msgstr ""
"shift_left (x, y), naficula x nipa y die-die si osi (isodipupo nipa 2 fun "
"bit)"

#: functions.py:392
msgid "shift_right(x, y), shift x by y bits to the right (divide by 2 per bit)"
msgstr ""
"shift_right (x, y), naficula x nipa y die-die si ọtun (pin nipa 2 fun bit)"

#: functions.py:397
msgid ""
"sin(x), return the sine of x. This is the y-coordinate on the unit circle at "
"the angle x"
msgstr "ese (x), pada lai ti x. Yi ni y-ipoidojuko lori kuro circle ni igun x"

#: functions.py:403
msgid ""
"sinh(x), return the hyperbolic sine of x. Given by (exp(x) - exp(-x)) / 2"
msgstr ""
"sinh (x), pada awọn hyperbolic lai ti x. Fun nipasẹ (exp (x) - exp (-x)) / 2"

#: functions.py:410
msgid "sinc(x), return the sinc of x. This is given by sin(x) / x."
msgstr "sinc (x), pada awọn sinc ti x. Eyi ni a fun nipasẹ ese (x) / x."

#: functions.py:415
msgid ""
"sqrt(x), return the square root of x. This is the value for which the square "
"equals x. Defined for x >= 0."
msgstr ""
"sqrt(x), pada awọn square root of x. Eleyi jẹ iye fun eyi ti awọn square je "
"egbe x. Telẹ fun x >= 0."

#: functions.py:420
msgid "square(x), return x * x"
msgstr "square (x), pada x * x"

#: functions.py:427
msgid "sub(x, y), return x - y"
msgstr "iha (x, y), pada x - y"

#: functions.py:432
msgid ""
"tan(x), return the tangent of x. This is the slope of the line from the "
"origin of the unit circle to the point on the unit circle defined by the "
"angle x. Given by sin(x) / cos(x)"
msgstr ""
"Tan (x), pada awọn tangent ti x. Yi ni ite ti ila lati awọn Oti ti kuro "
"Circle si ojuami lori kuro Circle asọye nipa awọn igun x. Fun nipasẹ ese (x) "
"/ nitori (x)"

#: functions.py:439
msgid "tanh(x), return the hyperbolic tangent of x. Given by sinh(x) / cosh(x)"
msgstr ""
"tanh (x), pada awọn hyperbolic tangent ti x. Fun nipasẹ sinh (x) / cosh (x)"

#: functions.py:444
msgid ""
"xor(x, y), logical xor. Returns True if either x is True (and y is False) or "
"y is True (and x is False), else returns False"
msgstr ""
"xor (x, y), mogbonwa xor. Pada Otitọ ti o ba ti boya x jẹ Otitọ (ati y jẹ "
"Eke) tabi y jẹ Otitọ (ati x jẹ Eke), miran pada Eke"

